@model IEnumerable<Baitap1.Models.PhanQuyen>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NhanVien)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CuaHang)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SanPham)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NamThang)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NhanVien.id_nhanvien)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CuaHang.id_cuahang)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SanPham.id_sanpham)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NamThang.id_namthang)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id_phanquyen">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id_phanquyen">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.id_phanquyen">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
